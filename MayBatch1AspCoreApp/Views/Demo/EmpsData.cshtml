@{
}

@model List<Emp>
<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach(var obj in Model)
    {
        <tr>
            <td>@obj.Id</td>
            <td>@obj.Name</td>
            <td>@obj.Salary</td>
        </tr>
    }
 
</table>

